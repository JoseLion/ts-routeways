{
  "name": "ts-routeways",
  "version": "0.0.0",
  "description": "Lightweight and type-safe! All-in-one solution to define and consume routes",
  "repository": "git@github.com:JoseLion/ts-routeways.git",
  "homepage": "https://joselion.github.io/ts-routeways/",
  "bugs": "https://github.com/JoseLion/ts-routeways/issues",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com> (https://github.com/JoseLion)",
  "license": "MIT",
  "keywords": [
    "codecs",
    "react",
    "querystring",
    "router",
    "routes",
    "routeways",
    "routing",
    "typesafe",
    "typescript"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "yarn compile && yarn lint && yarn test --forbid-only",
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "release": "semantic-release",
    "test": "NODE_ENV=test mocha \"test/**/*.test.ts?(x)\""
  },
  "dependencies": {
    "ts-pattern": "^4.2.2"
  },
  "devDependencies": {
    "@stackbuilders/assertive-ts": "^1.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.9.1",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "packageManager": "yarn@3.5.0"
