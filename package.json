{
  "name": "ts-routeways",
  "version": "0.0.0",
  "description": "Lightweight and type-safe! All-in-one solution to define and consume routes",
  "repository": "git@github.com:JoseLion/ts-routeways.git",
  "homepage": "https://joselion.github.io/ts-routeways/",
  "bugs": "https://github.com/JoseLion/ts-routeways/issues",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com> (https://github.com/JoseLion)",
  "license": "MIT",
  "keywords": [
    "codecs",
    "react",
    "querystring",
    "router",
    "routes",
    "routeways",
    "routing",
    "typesafe",
    "typescript"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "yarn compile && yarn lint && yarn test --forbid-only",
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "release": "semantic-release",
    "test": "NODE_ENV=test mocha \"test/**/*.test.ts?(x)\""
  },
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "ts-pattern": "^4.3.0"
  },
  "devDependencies": {
    "@stackbuilders/assertive-ts": "^1.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.2.4",
    "eslint-plugin-sonarjs": "^0.19.0",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "semantic-release": "^21.0.3",
    "semantic-release-yarn": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
