{
  "name": "ts-routeways",
  "version": "0.0.0",
  "description": "Lightweight and type-safe! All-in-one solution to define and consume routes",
  "repository": "git@github.com:JoseLion/ts-routeways.git",
  "homepage": "https://joselion.github.io/ts-routeways/",
  "bugs": "https://github.com/JoseLion/ts-routeways/issues",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com> (https://github.com/JoseLion)",
  "license": "MIT",
  "keywords": [
    "codecs",
    "querystring",
    "router",
    "routes",
    "routeways",
    "routing",
    "typesafe",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{ \"type\": \"commonjs\" }' > ./dist/cjs/package.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "check": "yarn compile && yarn lint && yarn test --run",
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "release": "semantic-release",
    "test": "NODE_ENV=test vitest"
  },
  "packageManager": "yarn@4.3.0",
  "dependencies": {
    "ts-pattern": "^5.2.0"
  },
  "devDependencies": {
    "@assertive-ts/core": "^2.1.0",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@stylistic/eslint-plugin": "^2.2.1",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.5",
    "eslint": "^9.5.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-extra-rules": "^0.0.0-development",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.12",
    "eslint-plugin-sonarjs": "^1.0.3",
    "semantic-release": "^24.0.0",
    "semantic-release-yarn": "^3.0.2",
    "tslib": "^2.6.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.13.1",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
