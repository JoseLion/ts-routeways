{
  "name": "ts-routeways",
  "version": "0.0.0",
  "description": "Lightweight and type-safe! All-in-one solution to define and consume routes",
  "repository": "git@github.com:JoseLion/ts-routeways.git",
  "homepage": "https://joselion.github.io/ts-routeways/",
  "bugs": "https://github.com/JoseLion/ts-routeways/issues",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com> (https://github.com/JoseLion)",
  "license": "MIT",
  "keywords": [
    "codecs",
    "react",
    "querystring",
    "router",
    "routes",
    "routeways",
    "routing",
    "typesafe",
    "typescript"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "yarn compile && yarn lint && yarn test --forbid-only",
    "compile": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json \"!(build|dist)/**/*.ts?(x)\"",
    "release": "semantic-release",
    "test": "NODE_ENV=test mocha \"test/**/*.test.ts?(x)\""
  },
  "dependencies": {
    "@cometlib/dedent": "^0.8.0-es.10"
  },
  "devDependencies": {
    "@stackbuilders/assertive-ts": "^1.1.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.15",
    "@types/react": "^18.0.18",
    "mocha": "^10.0.0",
    "react": "^18.2.0",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "typescript": "4.8.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "packageManager": "yarn@3.2.1"
}
