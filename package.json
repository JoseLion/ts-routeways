{
  "name": "ts-routeways",
  "version": "0.0.0",
  "description": "A type-safe solution to define and consume routes",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:JoseLion/ts-routeways.git",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "check": "yarn compile && yarn lint && yarn test --forbid-only",
    "lint": "tslint -c tslint.json \"!(dist)/**/*.ts?(x)\"",
    "test": "cross-env NODE_ENV=test TS_NODE_TRANSPILE_ONLY=true mocha \"test/**/*.test.ts?(x)\""
  },
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.32",
    "cross-env": "^7.0.3",
    "mocha": "^10.0.0",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  }
}
